@model DotNetCoreClientTest.Web2.Models.ResultTestModel

@{
    ViewData["Title"] = "RunScenario";
}

<h1>Run Scenario</h1>

<div><b>ID</b>: @Model.Id</div>
<div><b>Name</b>: @Model.Name</div>
<div><b>Result</b>: @Model.Result</div>
<div><b>Desc</b>: @Model.Desc</div>
<div><b>Error</b>: @Model.Error</div>

<div><b>Messages</b>:</div>

<ul>
    @{ foreach (var item in Model.Messages)
        {
            <li>@item</li>
        }}

</ul>